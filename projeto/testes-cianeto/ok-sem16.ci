@nce
class Milhar { 
    var Int numero;
    func getUser -> Int {
        self.numero = In.readInt; 
        self.numero = numero*1000;
        return self.numero;
    }
end

class Program {
    func run { 
        var Milhar numero; 
        numero = Milhar.new;
        Out.println: numero.getUser;        
    } 
end

